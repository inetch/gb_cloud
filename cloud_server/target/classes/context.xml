<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd
  http://www.springframework.org/schema/context
  https://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="dbmain" class="gb.cloud.server.db.DBSQLite">
        <!--<property name="connectionString" value="jdbc:sqlite:gb-cloud.db"/>
        <property name="className" value="org.sqlite.JDBC"/>-->
        <constructor-arg index="0" value="jdbc:sqlite:gb-cloud.db"/>
        <constructor-arg index="1" value="org.sqlite.JDBC"/>
    </bean>

</beans>